{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ccoskrnl",
            "type": "cppdbg",
            "request": "launch",
            "miDebuggerServerAddress": "127.0.0.1:8848",
            "program": "${workspaceFolder}/esp/EFI/ChengChengOS/ccoskrnl",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "logging": {
                "engineLogging": false
            },
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printting for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
            ],
            "launchCompleteCommand": "exec-continue"
        },
    ],
}
